lice\n                    };\n                    api.sendFormApi.send(params).then(response => {\n                        if (response.data.data.status != 'fail') {\n                            this.errors.email = response.data.errors;\n                        } else {\n                            this.errors.email = response.data.email\n                        }\n                    })\n                });\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .contacts_form {\n        background-image: url(\"../../assets/images/pattern.png\");\n    }\n</style>"],"sourceRoot":"src/components/Contacts"}]}